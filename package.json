{
  "name": "@aether-winds/service-base-ui",
  "version": "0.0.2",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "build:check": "tsc --noEmit",
    "dev": "nodemon --exec tsx src/index.ts",
    "format": "prettier --write \"**/*.{js,ts}\"",
    "format:check": "prettier --check \"**/*.{js,ts}\"",
    "lint": "eslint . --ext .js,.ts --format=compact",
    "prepublishOnly": "npm run build",
    "start": "node dist/index.js",
    "test": "mocha 'src/**/*.test.ts' --require tsx/cjs",
    "test:watch": "mocha 'src/**/*.test.ts' --require tsx/cjs --watch",
    "type-check": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aether-winds/service_base-ui.git"
  },
  "keywords": [
    "express",
    "typescript",
    "api",
    "service"
  ],
  "author": "Patrick Ortiz <pogithub@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aether-winds/service_base-ui/issues"
  },
  "homepage": "https://github.com/aether-winds/service_base-ui#readme",
  "description": "",
  "dependencies": {
    "express": "5.1.0",
    "hbs": "4.2.0"
  },
  "devDependencies": {
    "@eslint/js": "9.33.0",
    "@types/chai": "5.2.2",
    "@types/express": "5.0.3",
    "@types/hbs": "4.0.5",
    "@types/mocha": "10.0.10",
    "@types/node": "24.2.1",
    "@types/sinon": "17.0.4",
    "chai": "5.2.1",
    "eslint": "9.33.0",
    "eslint-formatter-compact": "^8.40.0",
    "globals": "16.3.0",
    "mocha": "11.7.1",
    "nodemon": "3.1.10",
    "prettier": "3.6.2",
    "sinon": "21.0.0",
    "tsx": "4.20.3",
    "typescript": "5.9.2",
    "typescript-eslint": "8.39.0"
  },
  "engines": {
    "node": "24.5.0",
    "npm": "11.5.1"
  }
}
