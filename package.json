{
  "name": "service_base-ui",
  "version": "0.0.1",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "dev": "nodemon --exec tsx src/index.ts",
    "start": "node dist/index.js",
    "lint": "eslint . --ext .js,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.ts,.tsx --fix",
    "format": "prettier --write \"**/*.{js,ts,tsx,json,md,html,css,scss}\"",
    "format:check": "prettier --check \"**/*.{js,ts,tsx,json,md,html,css,scss}\"",
    "type-check": "tsc --noEmit",
    "test": "mocha 'src/**/*.test.ts' --require tsx/cjs",
    "test:watch": "mocha 'src/**/*.test.ts' --require tsx/cjs --watch",
    "audit:fix": "npm audit fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aether-winds/service_base-ui.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/aether-winds/service_base-ui/issues"
  },
  "homepage": "https://github.com/aether-winds/service_base-ui#readme",
  "description": "",
  "dependencies": {
    "express": "5.1.0",
    "hbs": "^4.2.0"
  },
  "devDependencies": {
    "@types/chai": "^5.2.2",
    "@types/express": "^5.0.3",
    "@types/hbs": "^4.0.4",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.5.4",
    "@types/sinon": "^17.0.4",
    "@typescript-eslint/eslint-plugin": "^8.5.0",
    "@typescript-eslint/parser": "^8.5.0",
    "chai": "5.2.1",
    "eslint": "^9.10.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "mocha": "11.7.1",
    "nodemon": "3.1.10",
    "prettier": "^3.3.3",
    "sinon": "21.0.0",
    "tsx": "4.20.0",
    "typescript": "5.9.2"
  }
}
